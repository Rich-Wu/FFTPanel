<%= form_for :cohort, url: cohort_edit_path(@cohort.id), method: :patch, html: {class: ["menu", "inputForm"]} do |f| %>
  <h2 class = "formHeader">Edit Cohort Info</h2>
  Name: <%= f.text_field :name, value: @cohort.name %>
  Start Date: <%= f.date_field :start_date, value: @cohort.start_date %>
  End Date: <%= f.date_field :end_date, value: @cohort.end_date %>
  <%= f.submit "Update Account" %>
<% end %>
